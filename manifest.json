{
	"manifest_version": 2,
  "name": "Translate Mark",
  "description": "This extension shows a Translate result for the text",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
